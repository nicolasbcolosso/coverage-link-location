<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enter_Full_Limits</fullName>
    <active>false</active>
    <errorConditionFormula>OR(Aggregate_Limit__c  &lt; 10,  Occurrence_Limit__c &lt;10)</errorConditionFormula>
    <errorMessage>Policy limits cannot be abbreviated; insert full values (ex. &quot;1,000,000&quot;)</errorMessage>
</ValidationRule>
