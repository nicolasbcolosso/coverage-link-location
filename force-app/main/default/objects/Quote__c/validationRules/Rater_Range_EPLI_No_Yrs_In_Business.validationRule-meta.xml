<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rater_Range_EPLI_No_Yrs_In_Business</fullName>
    <active>true</active>
    <errorConditionFormula>$Setup.Master_Validation__c.Account_Active__c &amp;&amp; OR(RecordType.DeveloperName = &apos;Scale_Received&apos;, 
RecordType.DeveloperName = &apos;Scale_Locked&apos;, RecordType.DeveloperName = &apos;Scale_Open&apos;) &amp;&amp;

OR(
 EPLI_Number_of_Years_In_Business_Factor__c &lt; 0.85,
 EPLI_Number_of_Years_In_Business_Factor__c &gt; 1.35)</errorConditionFormula>
    <errorMessage>EPLI Number of Years in Business (Factor) is not within the valid range</errorMessage>
</ValidationRule>
