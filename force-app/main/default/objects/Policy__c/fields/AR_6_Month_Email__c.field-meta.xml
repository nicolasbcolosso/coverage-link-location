<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AR_6_Month_Email__c</fullName>
    <description>Field checks to true when AR Policy is 183 days til Renewal. Used to Trigger Zap &quot;AR 6 Month Reachout&quot;</description>
    <formula>AND (
RecordType.Name = &quot;Alpharoot&quot;,
In_Force__c = true,
Days_Until_Renewal__c = 183)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>AR 6 Month Email</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
